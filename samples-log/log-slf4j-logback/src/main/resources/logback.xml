<!--
    debug 开启配置加载状态信息打印
    scan：检测日志被修改后重新加载
    scanPeriod：配置修改重新加载扫描周期，取值可以看ch.qos.logback.core.util.Duration.valueOf()
    packagingData：是否开启对于异常日志显示每个栈信息是在哪个jar包和版本
-->
<configuration debug="false" scan="false" scanPeriod="1 second" packagingData="false">
    <root level="off">
    </root>
</configuration>